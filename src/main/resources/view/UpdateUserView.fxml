<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" fillWidth="false" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="751.0" prefWidth="896.0" spacing="1.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label text="First Name" />
      <TextField fx:id="fname" />
      <Label fx:id="Fname" text="Last Name" />
      <TextField fx:id="lname" />
      <Label text="Username" />
      <TextField fx:id="username" editable="false" opacity="0.25" />
      <Label text="Password" />
      <TextField fx:id="password" />
      <HBox alignment="CENTER" prefHeight="40.0" prefWidth="200.0" spacing="20.0">
         <children>
            <Button fx:id="updateUser" mnemonicParsing="false" text="Update" />
            <Button fx:id="close" mnemonicParsing="false" text="Close" />
         </children>
      </HBox>
      <Label fx:id="message" />
   </children>
</VBox>
