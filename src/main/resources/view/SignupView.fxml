<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" fillWidth="false" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="600.0" prefWidth="700.0" spacing="2.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label layoutX="85.0" layoutY="56.0" prefHeight="17.0" prefWidth="250.0" text="First Name" />
      <TextField fx:id="fname" layoutX="85.0" layoutY="97.0" maxWidth="250.0" />
      <Label layoutX="85.0" layoutY="24.0" prefHeight="17.0" prefWidth="250.0" text="Last Name" />
      <TextField fx:id="lname" layoutX="85.0" layoutY="52.0" maxWidth="250.0" />
      <Label prefHeight="17.0" prefWidth="250.0" text="Username" />
      <TextField fx:id="username" maxWidth="250.0">
         <VBox.margin>
            <Insets bottom="5.0" />
         </VBox.margin></TextField>
      <Label prefWidth="250.0" text="Password" />
      <PasswordField fx:id="password" maxWidth="250.0">
         <VBox.margin>
            <Insets bottom="10.0" />
         </VBox.margin>
      </PasswordField>
      <HBox alignment="CENTER" prefHeight="40.0" prefWidth="200.0" spacing="20.0">
         <children>
            <Button fx:id="createUser" mnemonicParsing="false" text="Create user" />
            <Button fx:id="close" mnemonicParsing="false" text="Close" />
         </children>
      </HBox>
      <Label fx:id="status" />
   </children>
</VBox>
